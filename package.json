{
  "main": "dist",
  "name": "example-tabris",
  "private": true,
  "scripts": {
    "test": "npm run build && npm run lint",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "build": "rollup -c",
    "watch": "rollup -c -w -m",
    "start": "tabris serve -a -w",
    "laun": "source ~/.zshrc && ANDROID_SDK_ROOT=~/Library/Android/sdk tabris run android"
  },
  "dependencies": {
    "happy-dom": "^2.11.2",
    "tabris": "~3.8.0",
    "vm-browserify": "^1.1.2",
    "vm-shim": "0.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "buffer": "^6.0.3",
    "elm": "^0.19.1-3",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "node-elm-compiler": "^5.0.5",
    "rollup": "^2.70.2",
    "rollup-pluginutils": "^2.8.2",
    "tabris-cli": "^3.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  }
}
